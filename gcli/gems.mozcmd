[{
  name: "gems",
  description: "search gems",
  runAt: "client",
  params: [{
    name: 'word',
    type: 'string',
    description: 'search word',
  }],
  exec: function(args, context) {
    let chromeWin = context.environment.chromeWindow || context.environment.chromeDocument.defaultView;
    let url = 'http://rubygems.org/search?utf8=%E2%9C%93&query=' + args.word;
    chromeWin.gBrowser.addTab(url);
  }
}]
